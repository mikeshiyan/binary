{
    "name": "shiyan/binary",
    "description": "Binary strings/files parsing and composing.",
    "keywords": ["binary", "files", "strings", "php", "gmp", "oop"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Shiyan",
            "homepage": "https://github.com/mikeshiyan"
        }
    ],
    "support": {
        "source": "https://github.com/mikeshiyan/binary",
        "issues": "https://github.com/mikeshiyan/binary/issues"
    },
    "require": {
        "php": "^7.1",
        "ext-gmp": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Shiyan\\Binary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shiyan\\Binary\\tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-text --whitelist src tests"
    }
}
